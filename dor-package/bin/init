#!/usr/bin/env node

const { program } = require('commander');
const package = require('../package');

program
    .option('--init', 'helps to init your app')
    .version(package.version)
    .on('--help', () => {
        console.log('A package by @alkaitagi. For initial project setup run "prepos-cli --init"');
    })
    .parse()
